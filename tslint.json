{
  "extends": [
    "tslint:recommended",
    "tslint-react-hooks"
  ],
  "rules": {
    "exhaustive-deps": false,
    "no-unused-variable": true,
    "react-hooks-nesting": "warning",
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-console": [
      false,
      "log"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "max-classes-per-file": false,
    "member-access": false,
    "no-unused-expression": false,
    "max-line-length": false
  }
}